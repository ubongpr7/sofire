{
    "version":2,
    "builds":[
        {

            "src":"core/wsgi.py",
            "use":"@vercel/python",
            "config":{
                "maxLamdaSize":"15mb",
                "untime":"python3.9"
            }  
        },
        {
            "src": "build.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$l"
      },
      {
        "src":"/(.*)",
        "dest":"core.wsgi.py"
      }
    ]
}
